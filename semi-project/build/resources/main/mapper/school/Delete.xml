<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kimbaekjung.semiproject.school.model.DeleteDAO">

    <update id="delete" parameterType="com.kimbaekjung.semiproject.school.dto.DeleteDTO">
        UPDATE student
        SET STUDENT_STATUS = 'N'
        WHERE STUDENT_CODE = #{studentCode};
    </update>



</mapper>