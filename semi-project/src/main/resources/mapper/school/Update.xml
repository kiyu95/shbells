<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kimbaekjung.semiproject.school.model.UpdateDAO">

    <update id="update" parameterType="com.kimbaekjung.semiproject.school.dto.UpdateDTO">
        UPDATE student
        SET STUDENT_NAME = #{studentName}
        WHERE STUDENT_CODE = #{studentCode};
    </update>



</mapper>